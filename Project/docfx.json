{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/**.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif",
          "**/*.svg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/project/breadcrumb/toc.json",
      "extendBreadcrumb": "true",
      "uhfHeaderId": "MSDocsHeader-M365-IT",
      "titleSuffix": "Project Server",
      "contributors_to_exclude": [
        "denisebmsft"
      ],
      "searchScope": [
        "Project Server"
      ],
      "feedback_system": "None",
      "hideEdit": true
    },
    "fileMetadata": {},
    "template": [],
    "dest": "OfficeDocs-ProjectServer",
    "markdownEngineName": "markdig"
  }
}
